<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity UnitOfMeasure.
    -->
    <changeSet id="20191215071736-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="unit_of_measure_id"
                                 baseTableName="unit_of_measure_limits"
                                 constraintName="fk_unit_of_measure_limits_unit_of_measure_id"
                                 referencedColumnNames="id"
                                 referencedTableName="unit_of_measure"/>

        <addForeignKeyConstraint baseColumnNames="limits_id"
                                 baseTableName="unit_of_measure_limits"
                                 constraintName="fk_unit_of_measure_limits_limits_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_limit"/>
        
    </changeSet>
</databaseChangeLog>
